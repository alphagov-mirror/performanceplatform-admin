{  
  "query": {
    "sort_by": "_timestamp:ascending"
  },
  "visualisation" : {
    "axes": {
        "x": {
            "format": "date", 
            "key": [
                "_start_at", 
                "_end_at"
            ], 
            "label": "Date"
        }, 
        "y": [
            {
                "format": {
                    "pence": true,
                    "type": "currency"
                },
                "label": "Average value"
            }
        ]
    }, 
    "axis-period": "month",
    "date-picker": {
        "start-date": "2016-08-29"
    },
    "format-options": {
        "dps": 5,
        "magnitude": true,
        "sigfigs": 5,
        "type": "number"
    },
    "value-attribute": "rate"
  }
}
